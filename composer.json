{
    "name": "nacosvel/laravel-database-manager",
    "description": "Nacosvel Database Manager Implementation for Laravel.",
    "keywords": [
        "Nacosvel Database Manager Implementation for Laravel",
        "Nacosvel Database Manager"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\DatabaseManager\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Nacosvel\\DatabaseManager\\DatabaseManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
